FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1;

boundaryField
{
	inlet
	{
        type            zeroGradient;
	}
	
	outlet
	{
        type            zeroGradient;
	}
	
    sides
    {
        type            zeroGradient;
    }
    empty
    {
        type            empty;
    }
}

